# importing "array" for array creations
import array as arr

# creating an array with integer type
a = arr.array('i', [1, 2, 3])

# printing original array
print("The new created array is : ", end=" ")
for i in range(0, 3):
    print(a[i], end=" ")
print()

# creating an array with float type
b = arr.array('d', [2.5, 3.2, 3.3])

# printing original array
print("The new created array is : ", end=" ")
for i in range(0, 3):
    print(b[i], end=" ")
print()

# inserting an element into the first array
a.insert(1, 4)
print()
#adding an element using append()
b.append(4.4)
print("\n array after intersection: ",end="")
for i in (b):
    print(i, end="")
print()
print("access element is:", a[0])
print("access element is:", b[2])
import array as arr

# Create an integer array
array_num = arr.array('i', [1, 3, 5, 3, 7, 9, 3])
print(f"Original array: {array_num}")

# Count occurrences of a specific number
count_3 = array_num.count(3)
print(f"Number of occurrences of the number 3: {count_3}")

# Reverse the array
array_num.reverse()
print("Reversed array:", array_num)