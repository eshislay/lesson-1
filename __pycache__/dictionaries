print("Dictionaries and it's Operations")
dict1={"Name":"Eshal","Course":"Python","Age":12}
print(dict1)
dict1["Age"]=13
print(dict1)
dict1["Grade"]="7th"
print(dict1)
print(dict1.keys())
print(dict1.values())
print(dict1.items())
print(dict1.get("Name"))
print(dict1.get("Age"))
print(dict1.get("Grade"))
print(dict1.pop("Age"))
print(dict1.popitem())
print(dict1.clear())